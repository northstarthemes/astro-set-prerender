{
  "name": "@northstarthemes/astro-set-prerender",
  "type": "module",
  "version": "0.0.2",
  "description": "Prerender plugin for Astro by North Star Themes",
  "author": "North Star Themes <northstarthemesofficial@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/northstarthemes/astro-set-prerender#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/northstarthemes/astro-set-prerender.git"
  },
  "bugs": "https://github.com/northstarthemes/astro-set-prerender/issues",
  "keywords": [],
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "astro": "^5.9.2",
    "tsdown": "^0.12.7",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typesVersions": {
      "*": {
        ".": [
          "./dist/index.d.ts"
        ]
      }
    }
  }
